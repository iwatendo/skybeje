{
  "name": "skybeje",
  "version": "0.8.0",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iwatendo/skybeje"
  },
  "scripts": {
    "start": "http-server ./dist/ -p 8080 -c-1",
    "dev": "npm-run-all -s webpack copy:lib copy:image copy:src setversion",
    "prod": "npm-run-all -s clean webpack:release copy:lib copy:image copy:src setversion",
    "setversion": "node dev/SetVersion.js",
    "clean": "rimraf dist/*",
    "webpack": "webpack --config ./webpack.config.dev.js",
    "webpack:release": "webpack --config ./webpack.config.release.js",
    "copy:lib": "cpx \"./src/Page/Lib/*\" ./dist/lib",
    "copy:image": "cpx \"./src/Page/Image/*\" ./dist/image",
    "copy:src": "cpx \"./src/Page/**/{*.html,*.css,*.map,*.json}\" ./dist"
  },
  "dependencies": {
    "co": "^4.6.0",
    "cpx": "^1.5.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "peerjs": "^1.4.7",
    "platform": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "youtube": "^0.1.0"
  },
  "devDependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "@types/jquery": "^3.5.14",
    "@types/platform": "^1.3.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/youtube": "^0.0.47"
  }
}